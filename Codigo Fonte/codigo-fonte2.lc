/* Teste cobrindo todas as especificacoes da linguagem LC */
int ;
string nome;
boolean naoTerminou;
final MAXITER=10;
{ Bloco Principal }
begin
  write,"Digite seu nome:";
  readln,nome;
  naoTerminou=true;
  n=0;
  while naoTerminou begin
    writeln,"Ola' ",nome;
    n=n+1;
  end
  if ( (1+3+5+7+9)<(2+4+6+8+10) ) begin
    nome="Natan";
  end
  else begin
    nome="Mateus";
  end
  n=1+2*3/2+(10-2);
end